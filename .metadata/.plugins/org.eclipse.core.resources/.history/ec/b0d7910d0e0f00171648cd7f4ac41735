
/**
 * 泛型的必要性
 * 
 * */

package com.Generic;

public class Generic_Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class Dog{
	private String name;
	private int age;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}

