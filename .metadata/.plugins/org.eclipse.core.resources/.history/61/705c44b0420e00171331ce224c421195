package com.Set;
//先引入包
import java.util.*;

public class ArrayListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//定义一个ArrayList对象
		ArrayList arrayList = new ArrayList();
		//显示大小
		System.out.println("array大小:" + arrayList.size());
		//像arraylist加入数据(类型是Object)
		
		
	}
}

class Clerk{
	private String name;
	private int age;
	private float value;
	public Clerk(String name1,int age,float value) {
		this.name = name1;
		this.age = age;
		this.value = value;
	}
}
