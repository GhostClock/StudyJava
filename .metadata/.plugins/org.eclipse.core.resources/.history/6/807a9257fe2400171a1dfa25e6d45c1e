/***
 * 
 * 演示线程使用的注意事项
 * 
 * 
 * */

package com.test04;

public class Demo01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Cat cat = new Cat();
		cat.run();
	}

}

class Dog implements Runnable{

	@Override
	public void run() {
		
		System.out.println("Dog Run");
	}
	
}

class Cat extends Thread{
	public void run() {
		System.out.println("Cat Run");
	}
}