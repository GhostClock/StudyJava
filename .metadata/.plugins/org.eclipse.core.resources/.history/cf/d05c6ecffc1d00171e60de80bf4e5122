/**
 * 演示如何通过继承Thread来开发线程
 * 
 * */

package com.test01;

public class Demo01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//创建一个对象
		Cat cat = new Cat();
		//启动线程
		cat.start();
	}

}

class Cat extends Thread{
	//重写run函数
	
	@Override
	public void run() {
		super.run();
		
		System.out.println("hello World");
	}
}
