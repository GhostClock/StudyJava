	MyPanel myPanel = null;
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

//定义自己的面板
class MyPanel extends JPanel{
	@Override
	public void paint(Graphics g) {
		// TODO Auto-generated method stub
		super.paint(g);
		g.fillOval(10, 10, 20, 20);
		
	}
}
