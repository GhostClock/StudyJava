//我的面板
class MyPanel extends JPanel{
	//抽象paint
	@Override
	public void paint(Graphics g) {
		// TODO Auto-generated method stub
		super.paint(g);
		
	}
}


//定义一个坦克类
class Tank {
	int x = 0;//横坐标
	int y = 0;//纵坐标
	public Tank(int x,int y) {
		this.x = x;
		this.y = y;
	}
}

class Hero extends Tank{
	public Hero(int x,int y) {
		super(x, y);
	}
}


